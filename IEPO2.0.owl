    <!-- http://www.iepo.com/iepo#Risperidone -->

    <owl:NamedIndividual rdf:about="http://www.iepo.com/iepo#Risperidone">
        <rdf:type rdf:resource="http://www.iepo.com/iepo#Drug"/>
        <BelongsTo rdf:resource="http://www.iepo.com/iepo#AbrahamJamesPR00000003"/>
        <BelongsTo rdf:resource="http://www.iepo.com/iepo#AbrahamJamesPR00000003SP1"/>
        <BelongsTo rdf:resource="http://www.iepo.com/iepo#DorcasHopePR00000004"/>
